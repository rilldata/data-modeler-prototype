{
  "name": "@rilldata/dev",
  "version": "0.10.0",
  "type": "module",
  "workspaces": [
    "web-cloud",
    "web-local"
  ],
  "scripts": {
    "build": "npm run build -w web-local",
    "build-vercel-pkg": "node dist/cli/build-vercel-pkg.js",
    "dev": "npm run dev -w web-local",
    "cli": "npm run cli -w web-local",
    "test": "npm run test -w web-local"
  },
  "bin": {
    "rill": "./dist/cli/data-modeler-cli.js"
  },
  "files": [
    "dist",
    "web-local/build-tools/post_install.cjs",
    "build",
    "web-local/static",
    "package.json",
    "package-lock.json",
    "LICENSE.md",
    "README.md"
  ],
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/**/*",
      "build/**/*",
      "package.json",
      "web-local/package.json"
    ]
  }
}
